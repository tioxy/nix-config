{
    "theme": "Ros√© Pine Dawn",
    "telemetry": {
        "diagnostics": false,
        "metrics": false
    },
    "vim_mode": false,
    "terminal": {
        "font_family": "IosevkaTerm Nerd Font",
        "font_size": 14,
        "blinking": "on"
    },
    "editor": {
        "font_family": "Iosevka Nerd Font",
        "font_size": 14,
        "font_weight": 450,
        "line_height": 1.5,
        "cursor_blink": true,
        "show_line_numbers": true,
        "tab_size": 2,
        "hard_tabs": false,
        "margin": 20,
        "scrollbar": false,
        "auto_pairs": true,
        "hover_popover_enabled": true
    },
    "buffer_font_size": 14,
    "autosave": "on_focus_change",
    "show_copilot_suggestions": false,
    "confirm_quit": false,
    "inlay_hints": {
        "enabled": true,
        "show_type_hints": true,
        "show_parameter_hints": true
    }
}
